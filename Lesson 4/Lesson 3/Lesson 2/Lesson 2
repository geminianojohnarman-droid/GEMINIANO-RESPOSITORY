MyGreetings = "Howdy, Noren!"
MyAge = 19

print(MyGreetings)
print(MyAge)

combine1 = str(MyAge) + MyGreetings
print(combine1)

combine2 = "wassup, Norelai!" + str(17)
print(combine2)


#TestCommands 2.0

a = input ("Enter your first number: ")
b = input ("Enter your second number: ")
c = input ("Enter your third number: ")

sumString = a+b+c
sumInt = int(c) + int(b)
print(sumString)
print(sumInt)
