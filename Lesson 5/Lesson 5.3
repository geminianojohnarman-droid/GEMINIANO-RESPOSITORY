#LIST

fruitsA = ["mango", "mango", "pineapple", "kiwi", "kiwi", "kiwi", "papaya"]
print(fruitsA)
print(fruitsA[3])
print(fruitsA.count("kiwi"))

isthere1 = "durian" in fruitsA
isthere2 = "mango" in fruitsA
print(isthere1)
print(isthere2)

fruitsA.append("cantaloupe")
print(fruitsA)

fruitsA.insert(4, "durian")
print(fruitsA)


#len = LENGTH
print(len(fruitsA))


#Count = counts the number of the character
print(fruitsA.count("mango"))
print(fruitsA.count("papaya"))


#Remove = removes the first match value
fruitsA.remove("cantaloupe")
print(fruitsA)


